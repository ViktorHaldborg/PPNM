set term svg background rgb "white"
set grid
set border
set xlabel "x"
set ylabel "y"
set key top right
set out "Time.svg"
set yrange [-40:50]
set xrange [-1.5:6]
set linestyle  1 lt 2 lw 2


plot "Time.txt" u 1:2 every 1::0::10000 pt 7 ps 0.25 t "Secular equation","Time.txt" u 1:3 w l lc 4 t "Secular reexpressed","Time.txt" u 1:4 w l t "Derivative reexpressed
